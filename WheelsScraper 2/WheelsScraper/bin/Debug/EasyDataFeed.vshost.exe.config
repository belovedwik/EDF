<?xml version="1.0"?>
<configuration>
	<configSections>
		<section name="log4net" type="System.Configuration.IgnoreSectionHandler"/>
		<sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="WheelsScraper.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
		</sectionGroup>
	</configSections>
	<runtime>
		<loadFromRemoteSources enabled="true"/>
	</runtime>

	<startup useLegacyV2RuntimeActivationPolicy="true">
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
	</startup>

	<log4net>
		<!-- Define some output appenders -->
		<appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
			<param name="File" value="logfiles\log"/>
			<appendToFile value="true"/>
			<rollingStyle value="Date"/>
			<datePattern value="yyyyMMdd"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline"/>
			</layout>
		</appender>
		<root>
			<appender-ref ref="RollingLogFileAppender"/>
			<level value="ALL"/>
		</root>
		<!-- Specify the level for some specific categories -->
	</log4net>

	<system.serviceModel>
		<bindings>
			<basicHttpBinding>
				<binding name="BasicHttpBinding_IUpdaterService" maxReceivedMessageSize="20480000" />
				<binding name="sceApiSoap" />
				<binding name="ws" maxBufferSize="655360" maxReceivedMessageSize="655360" />
			</basicHttpBinding>
		</bindings>
		<client>
			<endpoint address="http://service.easydatafeed.com/EDFServer/UpdaterService.svc"
			 binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUpdaterService"
			 contract="SRUpdater.IUpdaterService" name="BasicHttpBinding_IUpdaterService" />
			<endpoint address="http://api.shoppingcartelite.com/master/sceapi.asmx"
			 binding="basicHttpBinding" bindingConfiguration="sceApiSoap"
			 contract="srSCE.sceApiSoap" name="sceApiSoap" />
			<endpoint address="http://service.easydatafeed.com/EDFServer/AuthService.svc"
			 binding="basicHttpBinding" bindingConfiguration="ws" contract="srAuth.IAuthService"
			 name="ws" />
		</client>
	</system.serviceModel>
	<applicationSettings>
		<WheelsScraper.Properties.Settings>
			<setting name="DisableAutoupdate" serializeAs="String">
				<value>False</value>
			</setting>
			<setting name="EasyDataFeed_srSCE2_sceApi" serializeAs="String">
				<value>http://api.shoppingcartelite.com/master/sceapi.asmx</value>
			</setting>
		</WheelsScraper.Properties.Settings>
	</applicationSettings>
	<system.net>
		<settings>
			<servicePointManager expect100Continue="false" />
		</settings>
	</system.net>
</configuration>